class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        a=len(nums)
        b=range(0,a)
        for c in b:
            a=nums[:]
            x=int(target-nums[c])
            a[c]=-9992323232
            if x in a:
                d=nums.index(x)  
                f=sorted([c,d])
        return f
